{
    "manifest_version": 3,
    "name": "AlertSaveJob",
    "version": "1.0",
    "permissions": [
      "notifications",
      "storage",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "icons": {
      "128": "logo.png"
    }
  }
  